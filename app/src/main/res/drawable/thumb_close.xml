<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval" >

    <!--设置圆的半径-->
    <size android:height="@dimen/switch_diameter" android:width="@dimen/switch_diameter"/>

    <!--设置圆空心部分的背景色 -->
    <solid android:color="#eeeeee"/>

    <!--设置圆边的背景色-->
    <stroke
        android:width="1dp"
        android:color="#CCCECC"/>
</shape>